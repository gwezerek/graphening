<form class="filter">
  <fieldset class="filter__fieldset">
    <select class="filter__select--multi" id="filter__select--multi--set" multiple="multiple" placeholder="Filter by sets...">
    </select>
  </fieldset>
  <fieldset class="filter__fieldset">
    <select class="filter__select--multi" id="filter__select--multi--rarity" multiple="multiple" placeholder="Filter by rarity...">
      <option value="Common">Common</option>
      <option value="Uncommon">Uncommon</option>
      <option value="Rare">Rare</option>
      <option value="Mythic Rare">Mythic Rare</option>
      <option value="Special">Special</option>
      <option value="Basic Land">Basic Land</option>
    </select>
  </fieldset>
  <fieldset class="filter__fieldset">
    <select class="filter__select--multi" id="filter__select--multi--types" multiple="multiple" placeholder="Filter by type...">
    </select>
  </fieldset>
  <fieldset class="filter__fieldset">
    <select class="filter__select--multi" id="filter__select--multi--subtypes" multiple="multiple" placeholder="Filter by subtype...">
    </select>
  </fieldset>
  <fieldset class="filter__fieldset">
    <select class="filter__select--multi" id="filter__select--multi--artist" multiple="multiple" placeholder="Filter by artist...">
    </select>
  </fieldset>
</form>
